
<h2>Search for lastName: {{searchTerm}}</h2>
{{#if employees}}

<table border="1">
    <thead>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Emp ID</th>
    </thead>
    <tbody>
    {{#each employees}}
        <tr>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{id}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
{{else}}
    <h3>No Employees found!</h3>
{{/if}}
